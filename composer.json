{
    "name": "horde/xml_element",
    "description": "XML element library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Xml_Element",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        }
    ],
    "time": "2022-11-05",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0",
        "horde/util": "^3 || dev-FRAMEWORK_6_0",
        "ext-dom": "*"
    },
    "require-dev": {},
    "suggest": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0"
    },
    "autoload": {
        "psr-0": {
            "Horde_Xml_Element": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Xml\\Element\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}